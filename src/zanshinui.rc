<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<kpartgui name="zanshin" version="1">
 <MenuBar>
  <Menu name="file">
   <Action name="sidebar_new" />
   <Action name="sidebar_remove" />
   <Separator />
   <Action name="editor_add_action" />
   <Action name="editor_remove_action" />
   <Action name="editor_move_action" />
   <Action name="editor_promote_action" />
   <Action name="editor_dissociate_action" />
   <Separator />
   <Action name="synchronize_all" />
